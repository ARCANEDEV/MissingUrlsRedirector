{
    "name":        "arcanedev/missing-urls-redirector",
    "description": "Redirect the missing urls/pages in your Laravel application",
    "keywords":    ["arcanedev", "laravel", "redirect", "redirector", "url", "route", "routing", "seo", "missing page"],
    "homepage":    "https://github.com/ARCANEDEV/MissingUrlsRedirector",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               "^7.2.0",
        "arcanedev/support": "^5.1"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit":     "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\MissingUrlsRedirector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\MissingUrlsRedirector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\MissingUrlsRedirector\\RouteRedirectorServiceProvider",
                "Arcanedev\\MissingUrlsRedirector\\DeferredServicesProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
